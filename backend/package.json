{
  "name": "express-backend",
  "version": "0.1.0",
  "main": "./build/server.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "jest --coverage --colors",
    "test:watch": "jest --coverage --colors --watchAll",
    "start": "webpack --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!server/server.ts",
      "!server/app.ts",
      "!scripts/*"
    ]
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^5.16.0",
    "start-server-webpack-plugin": "^2.2.5",
    "typescript": "^3.4.5",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/mongodb": "^3.1.25",
    "@types/mongoose": "^5.3.27",
    "@types/node": "^11.13.8",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-router": "^4.4.5",
    "access-log": "^0.4.1",
    "bcrypt": "^3.0.6",
    "connect-mongo": "^3.0.0",
    "cors": "^2.8.5",
    "express-session": "^1.16.2",
    "http": "0.0.0",
    "mongodb": "^3.2.3",
    "mongodb-core": "^3.2.3",
    "mongoose": "^5.6.4",
    "multer": "^1.4.1",
    "node-schedule": "^1.3.2",
    "nodemon-webpack-plugin": "^4.0.8",
    "papaparse": "^4.6.3",
    "parseurl": "^1.3.3",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.0",
    "resolve-path": "^1.4.0",
    "socket.io": "^2.5.0"
  },
  "description": "Contacts search",
  "author": "RGB",
  "license": "ISC"
}
